{
  "non-interactive": false,
  "dry-run": false,
  "verbose": true,
  "force": false,
  "pkgFiles": [
    "package.json"
  ],
  "increment": "patch",
  "commitMessage": "Release v%s",
  "tagName": "v%s",
  "tagAnnotation": "Release v%s",
  "publish": true,
  "publishPath": ".",
  "githubRelease": true,
  "githubReleaseName": "Release %s",
  "githubReleaseBodyCommand": "git log --pretty=format:'* %s (%h)' [REV_RANGE]"
}